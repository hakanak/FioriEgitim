    <Table>
    <columns>
      <Column
        width="12em">
        <Text text="ID" />
      </Column>

       <Column
        minScreenWidth="Tablet"
        demandPopin="true">
        <Text text="AdÄ±" />
      </Column>

      
      <Column
        width="12em">
        <Text text="Tarih" />
      </Column>


    </columns>


    <items>
      <ColumnListItem>
        <cells>
          <ObjectIdentifier
            title="{ID}"
            text="{Adi}"
            class="sapUiTinyMarginTopBottom" />
          <Text
            text="{Tarih}" />
        </cells>
      </ColumnListItem>
    </items>



</Table>



</mvc:View>